    <body>

    {% block navbar %}
        <nav class="navbar navbar-dark navbar-fixed-top deep-purple">
            <button class="navbar-toggler hidden-sm-up" type="button" data-toggle="collapse" data-target="#collapseEx">
                <i class="fa fa-bars"></i></button>
            <div class="container">
                <div class="collapse navbar-toggleable-xs" id="collapseEx">
                    <ul class="nav navbar-nav">
                        <a href="{{ url('homepage') }}" class="navbar-brand">
                            <img src="{{ asset('assets/img/Logo3.png') }}" style="max-height: 30px" alt="JestemGraczem Logo">
                            JestemGraczem.pl
                        </a>
                        {#<li class="nav-item btn-group">#}
                            {#<a class="nav-link dropdown-toggle" id="dropdownTurnieje" data-toggle="dropdown"#}
                               {#aria-haspopup="true" aria-expanded="false">Turnieje</a>#}
                            {#<div class="dropdown-menu" aria-labelledby="dropdownTurnieje">#}
                                {#<a href="{{ url('tournament') }}" class="dropdown-item">#}
                                    {#<i class="fa fa-trophy" aria-hidden="true"></i>#}
                                    {#{{ 'nav.turnaments'|trans }}#}
                                {#</a>#}
                                {#<a href="{{ url('team') }}" class="dropdown-item">#}
                                    {#<i class="fa fa-users" aria-hidden="true"></i>#}
                                    {#{{ 'nav.teams'|trans }}#}
                                {#</a>#}
                            {#</div>#}
                        {#</li>#}
                        <li class="nav-item btn-group">
                            <a class="nav-link dropdown-toggle" id="dropdownGaming" data-toggle="dropdown"
                               aria-haspopup="true" aria-expanded="false">Gaming</a>
                            <div class="dropdown-menu" aria-labelledby="dropdownGaming">
                                <a href="{{ url('articles') }}" class="dropdown-item">
                                    <i class="fa fa-newspaper-o" aria-hidden="true"></i>
                                    {{ 'nav.news'|trans }}
                                </a>
                                <a href="{{ url('rss') }}" class="dropdown-item">
                                    <i class="fa fa-rss-square" aria-hidden="true"></i>
                                    {{ 'nav.rss'|trans }}
                                </a>
                                <a href="{{ url('status') }}" class="dropdown-item">
                                    <i class="fa fa-power-off" aria-hidden="true"></i>
                                    {{ 'nav.status'|trans }}
                                </a>
                                <a href="{{ url('centrum') }}" class="dropdown-item">
                                    <i class="fa fa-gamepad" aria-hidden="true"></i>
                                    {{ 'nav.center'|trans }}
                                </a>
                            </div>
                        </li>
                        <li class="nav-item btn-group">
                            <a class="nav-link dropdown-toggle" id="dropdownFun" data-toggle="dropdown"
                               aria-haspopup="true" aria-expanded="false">Fun</a>
                            <div class="dropdown-menu" aria-labelledby="dropdownFun">
                                <a href="{{ url('mem') }}" class="dropdown-item">
                                    <i class="fa fa-picture-o" aria-hidden="true"></i> {{ 'nav.meme'|trans }}
                                </a>
                                <a href="{{ url('stream') }}" class="dropdown-item">
                                    <i class="fa fa-twitch" aria-hidden="true"></i> {{ 'nav.streams'|trans }}
                                </a>
                                <a href="{{ url('video') }}" class="dropdown-item">
                                    <i class="fa fa-youtube-play" aria-hidden="true"></i> {{ 'nav.video'|trans }}
                                </a>
                            </div>
                        </li>
                    </ul>

                    <ul class="nav navbar-nav nav-flex-icons">
                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                            <li class="nav-item btn-group">
                                <a class="nav-link dropdown-toggle" id="dropdownUser" data-toggle="dropdown"
                                   aria-haspopup="true" aria-expanded="false">
                                    <i class="fa fa-user"></i>
                                    {{ app.user.username }}
                                </a>
                                <div class="dropdown-menu" aria-labelledby="dropdownUser">
                                    {% if is_granted('ROLE_ADMIN') %}
                                        <a href="{{ url('easyadmin') }}" target="_blank">
                                            <i class="fa fa-terminal" aria-hidden="true"></i> AdminCP
                                        </a>
                                    {% endif %}
                                    <a class="dropdown-item" href="{{ url('user',{'user': app.user.username}) }}">
                                        <i class="fa fa-user"
                                           aria-hidden="true"></i> {{ 'profile.publicprofil'|trans({}, 'FOSUserBundle') }}
                                    </a>
                                    <a class="dropdown-item" href="{{ url('fos_user_change_password') }}">
                                        <i class="fa fa-key"
                                           aria-hidden="true"></i> {{ 'change_password.submit'|trans({}, 'FOSUserBundle') }}
                                    </a>
                                    <a class="dropdown-item" href="{{ url('fos_user_profile_edit') }}">
                                        <i class="fa fa-pencil"
                                           aria-hidden="true"></i> {{ 'profile.edit.edit'|trans({}, 'FOSUserBundle') }}
                                    </a>
                                    <a class="dropdown-item" href="{{ url('fos_user_security_logout') }}">
                                        <i class="fa fa-sign-out"
                                           aria-hidden="true"></i> {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                                    </a>
                                </div>
                            </li>
                        {% else %}
                            <li class="nav-item btn-group">
                                <a class="nav-link dropdown-toggle" id="dropdownUser" data-toggle="dropdown"
                                   aria-haspopup="true" aria-expanded="false">
                                    <i class="fa fa-user"></i>
                                </a>
                                <div class="dropdown-menu" aria-labelledby="dropdownUser">
                                    <a class="dropdown-item" href="{{ url('fos_user_security_login') }}">
                                        {{ 'layout.login'|trans({}, 'FOSUserBundle') }}
                                    </a>
                                    <a class="dropdown-item" href="{{ url('fos_user_registration_register') }}">
                                        {{ 'account.register'|trans }} {{ 'account.now'|trans }}
                                    </a>
                                    <a class="dropdown-item" href="{{ url('fos_user_resetting_request') }}">
                                        {{ 'account.forgotpass'|trans }}
                                    </a>
                                </div>
                            </li>
                        {% endif %}
                        <li class="nav-item">
                            <a href="http://youtube.com/PanOtlet" target="_blank" class="nav-link waves-effect waves-light">
                                <i class="fa fa-youtube"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="http://twitter.com/PanOtlet" target="_blank" class="nav-link waves-effect waves-light">
                                <i class="fa fa-twitter"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="http://facebook.com/JestemGraczemPL" target="_blank" class="nav-link waves-effect waves-light">
                                <i class="fa fa-facebook"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    {% endblock %}

    {% block beforeBody %}{% endblock %}

    <section id="body">
        {% block body %}
