<!DOCTYPE html>
<html>
<head>
    {{ sonata_seo_metadatas() }}

    <title>{% block title %}Strona główna{% endblock %} :: JestemGraczem.pl</title>

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css') }}">
        {#<link rel="stylesheet" href="{{ asset('assets/css/bootstrap-theme.min.css') }}">#}
        <link rel="stylesheet" href="{{ asset('assets/css/material.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/ripples.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
    {% endblock %}

    {% block head %}{% endblock %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
<div id="background"></div>

{% include 'navbar.html.twig' %}

{% block body %}

    <main class="container">
        <div class="row">
            <div class="jumbotron col-md-8">
                <div class="row">
                    <div class="container-fluid">
                        {% block content %}{% endblock %}

                    </div>

                </div>
            </div>
            <div class="jumbotron col-md-3" style="margin-left: 20px">
                SIDEBAR
            </div>
        </div>
    </main>

{% endblock %}

{% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="{{ asset('assets/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('assets/js/material.min.js') }}"></script>
    <script src="{{ asset('assets/js/ripples.min.js') }}"></script>
    <script src="https://use.fontawesome.com/c0b3ee6d21.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.8.4/moment.min.js"></script>
    <script>
        $.material.init();
    </script>
{% endblock %}
</body>
</html>
